// Online C compiler to run C program online
#include <stdio.h>
int salarycalculator(int,int,int);
int main() {
    
    int ta=1000,da=1000,basic=1000,salary;
    salary = salarycalculator(ta,da,basic);
    printf("salary is %d",salary);

}
int salarycalcalator(int ta,int da,int basicsalary)
{
    int salary;
    salary=ta+da+basicsalary;   
    printf("salary is %d",salary);
   return salary;
}
